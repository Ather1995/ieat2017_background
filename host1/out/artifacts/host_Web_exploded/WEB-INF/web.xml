<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>servlet.user.UserLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/servlet/user/UserLoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserRegisterServlet</servlet-name>
        <servlet-class>servlet.user.UserRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRegisterServlet</servlet-name>
        <url-pattern>/servlet/user/UserRegisterServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserLoginOutServlet</servlet-name>
        <servlet-class>servlet.user.UserLoginOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoginOutServlet</servlet-name>
        <url-pattern>/servlet/user/UserLoginOutServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserUpdateInfoServlet</servlet-name>
        <servlet-class>servlet.user.UserUpdateInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserUpdateInfoServlet</servlet-name>
        <url-pattern>/servlet/user/UserUpdateInfoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ParseJSONServlet</servlet-name>
        <servlet-class>servlet.user.ParseJSONServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParseJSONServlet</servlet-name>
        <url-pattern>/servlet/user/ParseJSONServlet</url-pattern>
    </servlet-mapping>
</web-app>